@model BoardGames.Search.SearchResults

Found <span class="badge">@Model.TotalMatches</span> Results
<table class="table table-striped">
  <thead>
  <th></th>
  <th>Name</th>
  <th>Type</th>
  <th>Year</th>
  <th>Min Players</th>
  <th>Max Players</th>
  <th>Time</th>
  <th>Age</th>
  <th>Rating</th>
  <th>Owners</th>
  </thead>
  @foreach(var formula in Model.Items)
  {
      <tr>
        <td>@formula.Id</td>
        <td>@formula.Name</td>
        <td>@formula.GameType.Description</td>
        <td>@formula.YearPublished</td>
        <td>@formula.MinPlayers</td>
        <td>@formula.MaxPlayers</td>
        <td>@formula.PlayingTime</td>
        <td>@formula.MinAge</td>
        <td>@formula.AverageRating</td>
        <td>@formula.TotalOwners</td>
      </tr>
  }
</table>